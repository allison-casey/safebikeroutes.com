{
  "name": "safe-bike-routes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma migrate dev && kysely-codegen --url 'env(POSTGRES_URL)' && next build",
    "start": "next start",
    "lint:next": "next lint",
    "lint:exports": "npx ts-unused-exports tsconfig.json --excludePathsFromReport='.next' --searchNamespaces --showLineNumber",
    "lint:typecheck": "tsc --noEmit",
    "lint": "npm run lint:next && npm run biome && npm run lint:typecheck",
    "biome": "biome check",
    "format": "biome check --write",
    "test": "jest --watch",
    "migrate": "prisma migrate dev && npm run generate-types",
    "generate-types": "kysely-codegen --url 'env(POSTGRES_URL)'",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{css}": ["npx stylelint --fix"]
  },
  "dependencies": {
